{
  "$schema": "http://json.schemastore.org/template",
  "author": "daniilda",
  "classifications": [ "Common", "Library", "Package"],
  "identity": "ToxiCode.Templates.Package",
  "name": "ToxiCode Package",
  "shortName": "tc-pkg",
  "sourceName": "ToxiCode.Templates.Package",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols":{
    "framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
            "choice": "net6.0",
            "description": "Target .NET Core 6"
        }
      ],
      "defaultValue": "net6.0",
      "replaces": "net6.0"
    }
  }
}